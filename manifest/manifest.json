{
  "$schema": "https://statics.teams.microsoft.com/sdk/v1.2/manifest/MicrosoftTeams.schema.json",
  "manifestVersion": "1.2",
  "version": "1.0",
  "id": "<<REGISTERED_BOT_ID>>",
  "packageName": "com.microsoft.teams.samples.authbot",
  "developer": {
    "name": "Your Name Here",
    "websiteUrl": "https://www.example.com",
    "privacyUrl": "https://privacy.microsoft.com/PrivacyStatement",
    "termsOfUseUrl": "https://www.microsoft.com/en-us/servicesagreement/"
  },
  "name": {
    "short": "Teams AuthBot"
  },
  "description": {
    "short": "Bot authentication sample for Microsoft Teams",
    "full": "Bot authentication sample for Microsoft Teams"
  },
  "icons": {
    "outline": "Logo_LinkedIn_20x20.png",
    "color": "Logo_LinkedIn_96x96.png"
  },
  "accentColor": "#0077B5",
  "configurableTabs": [
  ],
  "staticTabs": [
  ],
  "bots": [
    {
      "botId": "<<REGISTERED_BOT_ID>>",
      "scopes": [
        "personal"
      ]
    }
  ],
  "permissions": [
    "messageTeamMembers",
    "identity"
  ],
  "validDomains": [
    "<<BASE_URI_DOMAIN>>"
  ]
}
